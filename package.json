{
  "name": "vitesse-webext",
  "displayName": "Vitesse WebExt",
  "version": "0.0.1",
  "type": "module",
  "private": true,
  "description": "[description]",
  "scripts": {
    "dev-chromium": "cross-env NODE_ENV=development esno scripts/esbuild.ts",
    "dev-firefox": "cross-env NODE_ENV=development EXTENSION=firefox esno scripts/esbuild.ts",
    "start:chromium": "web-ext run --target=chromium",
    "start:firefox": "web-ext run",
    "build": "cross-env NODE_ENV=production esno scripts/esbuild.ts",
    "build-firefox": "cross-env NODE_ENV=production EXTENSION=firefox esno scripts/esbuild.ts",
    "pack": "cross-env NODE_ENV=production run-p pack:*",
    "pack:zip": "rimraf dist/extension.zip && jszip-cli add dist/dev/* -o ./dist/extension.zip",
    "pack:crx": "crx pack dist/dev -o ./dist/extension.crx",
    "pack:xpi": "cross-env WEB_EXT_ARTIFACTS_DIR=./dist web-ext build",
    "clear": "rimraf extension/dist extension/manifest.json",
    "lint": "eslint --cache .",
    "test": "vitest test"
  },
  "devDependencies": {
    "@biomejs/biome": "1.4.1",
    "@craftamap/esbuild-plugin-html": "0.6.1",
    "@ffflorian/jszip-cli": "^3.5.7",
    "@iconify/json": "^2.2.161",
    "@svgr/core": "8.1.0",
    "@svgr/plugin-jsx": "8.1.0",
    "@types/fs-extra": "^11.0.4",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^18.19.3",
    "@types/webextension-polyfill": "^0.10.7",
    "@unocss/postcss": "0.58.0",
    "@unocss/reset": "^0.58.0",
    "@vue/test-utils": "^2.4.3",
    "@ziloen/eslint-config": "0.1.12",
    "core-js": "3.34.0",
    "cross-env": "^7.0.3",
    "crx": "^5.0.1",
    "esbuild": "0.19.10",
    "esbuild-style-plugin": "1.6.3",
    "eslint": "^8.56.0",
    "esno": "^4.0.0",
    "fs-extra": "^11.2.0",
    "jsdom": "^23.0.1",
    "kolorist": "^1.8.0",
    "npm-run-all": "^4.1.5",
    "rimraf": "^5.0.5",
    "ts-lib-enhance": "^1.0.16",
    "type-fest": "4.8.3",
    "typescript": "^5.3.3",
    "unocss": "^0.58.0",
    "unocss-preset-ziloen": "^0.0.5",
    "unplugin-auto-import": "^0.17.2",
    "unplugin-icons": "^0.18.1",
    "unplugin-vue": "4.5.2",
    "unplugin-vue-components": "^0.26.0",
    "unplugin-vue-jsx": "0.2.2",
    "vitest": "^1.1.0",
    "volar-service-prettyhtml": "^0.0.17",
    "vue": "^3.3.13",
    "web-ext": "^7.9.0",
    "webextension-polyfill": "^0.10.0"
  },
  "dependencies": {
    "@vueuse/core": "10.7.0",
    "@vueuse/rxjs": "^10.7.0",
    "@wai-ri/core": "^0.0.101",
    "clsx": "2.0.0",
    "lodash-es": "^4.17.21",
    "pinia": "^2.1.7",
    "rxjs": "^7.8.1",
    "serialize-error": "11.0.3",
    "ulid": "^2.3.0"
  },
  "webExt": {
    "sourceDir": "dist/dev",
    "artifactsDir": "dist",
    "ignoreFiles": [
      "package-lock.json",
      "yarn.lock",
      "pnpm-lock.yaml"
    ],
    "run": {
      "browserConsole": false,
      "firefoxPreview": [
        "mv3"
      ],
      "target": [
        "firefox-desktop"
      ]
    },
    "build": {
      "overwriteDest": true,
      "filename": "extension.xpi"
    }
  }
}