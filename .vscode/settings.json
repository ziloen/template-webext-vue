{
  "[typescript]": {
    "editor.defaultFormatter": "dbaeumer.vscode-eslint"
  },
  "editor.codeActionsOnSave": [
    // only sort imports
    "source.sortImports",
    // remove unused imports & sort imports
    // "source.organizeImports",
    "source.fixAll.eslint",
  ],
  "eslint.format.enable": true,
  "prettier.enable": false
}